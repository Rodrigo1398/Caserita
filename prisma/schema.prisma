generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Category {
  id String @id @default(uuid())
  name String
  image String
}

model Product {
  id String @id @default(uuid())
  slug String
  title String
  price Decimal
  state Boolean
  
}